{
  "name": "GQLZipCodeJP",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:sato1043/GQLZipCodeJP.git"
  },
  "license": "MIT",
  "author": "sato1043 <sato1043@updater.cc>",
  "copyright": "Copyright (c) 2024 sato1043@updater.cc",
  "private": true,
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  },
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "start": "exit 1",
    "dev": "dotenvx run -- tsx watch ./src",
    "lint": "run-s lint:*",
    "lint:md": "markdownlint-cli2 \"**/*.md\" && textlint \"**/*.md\"",
    "lint:prettier": "prettier --check .",
    "lint:ts": "tsc --noEmit",
    "lint:es": "eslint",
    "lint:dep": "depcruise src",
    "lint:pkgver-desc": "grep -E '\"[~^]' package.json; test $? -eq 1",
    "test": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules --no-warnings\" dotenvx run -- jest",
    "mnt:requisite:zipdata": "(cd scripts/zipcode-json/data ; bash _extract-data.sh)",
    "mnt:requisite:devcert": "(cd scripts/crt@localhost ; bash gencrt.sh)",
    "mnt:requisite:devenv": "(cd scripts/env@localhost ; bash genenv.sh)",
    "mnt:requisite:lefthook": "lefthook uninstall && lefthook install",
    "mnt:add-requisites": "run-s mnt:requisite:*",
    "mnt:api:keygen": "dotenvx run -- tsx scripts/keytool -e",
    "mnt:prettier": "prettier --write --list-different --no-error-on-unmatched-pattern",
    "mnt:doc:toc": "doctoc --github --title 'CONTENTS;' --maxlevel 3 README.md",
    "mnt:pkg:add": "npm install --save-exact",
    "mnt:pkg:add-dev": "npm install --save-dev --save-exact",
    "mnt:pkg:defcheck": "typesync --dry",
    "mnt:pkg:depcheck": "depcheck",
    "mnt:pkg:updcheck": "npx -p npm-check-updates --yes --prefer-online -- ncu --upgrade --install always --interactive",
    "rel:patch:rc": "commit-and-tag-version --release-as patch --prerelease",
    "rel:patch": "commit-and-tag-version --release-as patch",
    "rel:minor:rc": "commit-and-tag-version --release-as minor --prerelease",
    "rel:minor": "commit-and-tag-version --release-as minor",
    "rel:major:rc": "commit-and-tag-version --release-as major --prerelease",
    "rel:major": "commit-and-tag-version --release-as major"
  },
  "dependencies": {
    "@godaddy/terminus": "4.12.1",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "express": "4.19.2",
    "helmet": "7.1.0",
    "http-errors": "2.0.0",
    "lodash": "4.17.21",
    "morgan": "1.10.0",
    "uuid": "10.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.4.0",
    "@commitlint/config-conventional": "19.2.2",
    "@dotenvx/dotenvx": "1.7.0",
    "@eslint/js": "9.9.0",
    "@types/compression": "1.7.5",
    "@types/cookie-parser": "1.4.7",
    "@types/cors": "2.8.17",
    "@types/debug": "4.1.12",
    "@types/eslint": "9.6.0",
    "@types/eslint__js": "8.42.3",
    "@types/eslint-config-prettier": "6.11.3",
    "@types/express": "4.17.21",
    "@types/express-list-endpoints": "6.0.3",
    "@types/http-errors": "2.0.4",
    "@types/jest": "29.5.12",
    "@types/lodash": "4.17.7",
    "@types/morgan": "1.9.9",
    "@types/node": "22.2.0",
    "@types/supertest": "6.0.2",
    "@types/uuid": "10.0.0",
    "commit-and-tag-version": "12.4.1",
    "depcheck": "1.4.7",
    "dependency-cruiser": "16.3.10",
    "doctoc": "2.2.1",
    "eslint": "9.9.0",
    "eslint-config-prettier": "9.1.0",
    "express-list-endpoints": "7.1.0",
    "getopts": "2.3.0",
    "globals": "15.9.0",
    "jest": "29.7.0",
    "markdownlint-cli2": "0.13.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.3.3",
    "supertest": "7.0.0",
    "textlint": "14.0.5",
    "textlint-rule-preset-ja-technical-writing": "10.0.1",
    "textlint-rule-preset-japanese": "10.0.3",
    "ts-jest": "29.2.4",
    "tsx": "4.17.0",
    "typescript": "5.5.4",
    "typescript-eslint": "8.1.0",
    "typesync": "0.13.0"
  }
}
